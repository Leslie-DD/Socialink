            android:gravity="center_vertical"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"

            android:text="学院"

            />

        <TextView
            android:id="@+id/friendsetTip5"
            android:layout_width="wrap_content"
