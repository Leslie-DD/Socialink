<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/base_color_white"
    android:layout_height="match_parent"
    android:focusable="true"
    android:orientation="vertical"
    android:focusableInTouchMode="true">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout2">
        <include layout="@layout/new_title_bar2"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/linearLayout2"
        android:gravity="center_horizontal"
        android:id="@+id/headandsex"
        android:orientation="horizontal">
        <com.example.heshequ.view.CircleView
            android:layout_width="75dp"
            android:src="@mipmap/head3"
            android:id="@+id/ivHead"
            android:layout_height="75dp"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:id="@+id/xingbie"
            android:layout_gravity="bottom"
            android:src="@mipmap/me19" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/headandsex"
        android:id="@+id/nicheng">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#fff"
            android:drawablePadding="10dp"
            android:id="@+id/nicknametext"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"
            android:gravity="center"
            android:text=""

            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/nicheng"
        android:id="@+id/gexing">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#fff"
            android:drawablePadding="10dp"
            android:id="@+id/gexingqianming"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"
            android:gravity="center"
            android:text=""

            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/gexing"
        android:id="@+id/shengri">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#fff"
            android:drawablePadding="10dp"

            android:gravity="center_vertical"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"

            android:text="生日"

            />

        <TextView
            android:id="@+id/friendsetTip3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="11dp"
            android:layout_toStartOf="@+id/right3"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="#999999"
            />


        <ImageView
            android:id="@+id/right3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="11dp"
            android:src="@mipmap/selected" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/shengri"
        android:id="@+id/xuexiao">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#fff"
