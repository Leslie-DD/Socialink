            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="11dp"
            android:src="@mipmap/selected" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/xueyuan"
        android:id="@+id/zhuanye">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#fff"
            android:drawablePadding="10dp"

            android:gravity="center_vertical"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"

            android:text="专业"

            />

        <TextView
            android:id="@+id/friendsetTip6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="11dp"
            android:layout_toStartOf="@+id/right6"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="#999999"
            />


        <ImageView
            android:id="@+id/right6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="11dp"
            android:src="@mipmap/selected" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/zhuanye"
        android:id="@+id/nianji">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
