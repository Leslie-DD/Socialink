            android:gravity="center_vertical"
            android:paddingLeft="18dp"
            android:paddingRight="8dp"
            android:paddingTop="14dp"

            android:text="学校"

            />

        <TextView
            android:id="@+id/friendsetTip4"
            android:layout_width="wrap_content"
