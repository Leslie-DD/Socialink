        <RelativeLayout
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_width="match_parent"
            android:background="#f5f5f5"
            android:layout_height="180dp" >
            <ImageView
                android:layout_width="match_parent"
                android:src="@mipmap/mrtp"
                android:id="@+id/ivPic"
                android:scaleType="centerCrop"
                android:layout_height="match_parent" />
            <TextView
                style="@style/editor_style"
                android:id="@+id/tvPicEditor"
                android:visibility="gone"
                android:layout_alignParentRight="true"
                android:padding="15dp"
                />
        </RelativeLayout>


        <View
            android:layout_marginTop="10dp"
            style="@style/h_line_8"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:gravity="center_vertical"
            android:layout_height="50dp">
            <View
                android:layout_marginLeft="15dp"
                style="@style/view_4_17"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:textSize="14sp"
                android:textColor="#333333"
                android:text="团队介绍"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content" />
            <TextView
                style="@style/editor_style"
                android:id="@+id/tvTeamEditor"
                android:layout_alignParentRight="true"
                android:padding="15dp"
                android:visibility="gone"
                />
        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:id="@+id/tvIntroduction"
            android:lineSpacingExtra="2dp"
            android:letterSpacing="0.05"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:textColor="#333333"/>

        <View
            android:layout_marginTop="20dp"
            style="@style/h_line_8"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:gravity="center_vertical"
            android:layout_height="50dp">
            <View
                android:layout_marginLeft="15dp"
                style="@style/view_4_17"/>
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"
                android:textSize="14sp"
                android:textColor="#333333"
                android:text="他们说"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="wrap_content"
                android:textColor="#999999"
                android:textSize="12sp"
                android:text="更多"
                android:gravity="center_vertical"
                android:id="@+id/tvOtherMore"
                android:layout_marginRight="15dp"
                android:layout_height="match_parent" />
        </LinearLayout>
        <com.example.heshequ.view.MyLv
            android:layout_width="match_parent"
            android:id="@+id/lv"
            android:dividerHeight="1px"
            android:divider="#f5f5f5"
            android:layout_height="wrap_content">
        </com.example.heshequ.view.MyLv>
        <View
            android:layout_marginTop="20dp"
            style="@style/h_line_8"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:gravity="center_vertical"
            android:layout_height="50dp">
            <View
                android:layout_marginLeft="15dp"
                style="@style/view_4_17"/>
            <TextView
                android:layout_width="wrap_content"
                android:textSize="14sp"
                android:textColor="#333333"
                android:text="团建"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="wrap_content"
                android:textColor="#999999"
                android:textSize="12sp"
                android:text="编辑"
                android:visibility="gone"
                android:layout_marginLeft="15dp"
                android:id="@+id/tvTjEditor"
                android:layout_marginRight="15dp"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="0dp"
                android:layout_weight="1"

                android:textColor="#999999"
                android:textSize="12sp"
                android:text="更多"
                android:gravity="center_vertical|right"
                android:id="@+id/tvTjMore"
                android:layout_marginRight="15dp"
                android:layout_height="match_parent" />
        </LinearLayout>


        <com.example.heshequ.view.MyRecyclerview
            android:layout_width="match_parent"
            android:layout_margin="15dp"
            android:id="@+id/ivRecycview"
            android:scrollbars="none"

            android:layout_height="wrap_content">

        </com.example.heshequ.view.MyRecyclerview>
    </LinearLayout>
    </com.example.heshequ.view.MyScrollView>
